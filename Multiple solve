#include<stdio.h> //importing libraries
#include<conio.h> // it is your library file
int main()
{
clrscr();
int addition, substraction, multiply,num1,num2;
float division;
printf("Enter your number");
scanf("%d%d",&num1,&num2);
addition=num1 +num2;
printf("Addition=%d",addition);
substraction=num1-num2;
printf("Substraction=%d", substraction);
multiply=num1*num2;
printf("Multiplication= %d",multiply);
division=num1/num2;
printf("Division= %f", division");
getch();
return 0;
}
